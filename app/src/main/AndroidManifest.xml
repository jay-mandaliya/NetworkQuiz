<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.root.mobilequizz">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Admin" />
        <activity android:name=".Student" />
        <activity android:name=".AdminPanel" />
        <activity android:name=".ManageStudent" />
        <activity
            android:name=".StudentPanel"
            android:theme="@style/StudentToolbar" />
        <activity android:name=".ReportActivity" />
        <activity android:name=".ManageQuestions"></activity>
    </application>

</manifest>